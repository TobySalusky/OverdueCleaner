{
  "manifest_version": 2,
  "name": "Schoology Overdue Cleaner",
  "version": "1.0.0",
  "content_scripts": [
    {
      "matches": [
        "https://learn.lcps.org/*"
      ],
      "js": [
        "script.js"
      ],
      "css": [
        "styles.css"
      ]
    }
  ],
  "browser_action": {
    "default_icon": "icon.png"
  },
  "default_locale": "en",
  "description": "Clean up your screen by dismissing overdue Schoology assignments.",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "author": "Toby Salusky",
  "permissions": [
    "storage"
  ]
}
