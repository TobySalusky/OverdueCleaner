{
  "manifest_version": 2,
  "name": "Overdue Remover",
  "version": "0.1.0",
  "content_scripts": [
    {
      "matches": [
        "https://learn.lcps.org/*"
      ],
      "js": [
        "script.js"
      ],
      "css": [
        "styles.css"
      ]
    }
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "icon.png"
  },
  "permissions": [
    "storage"
  ]
}
